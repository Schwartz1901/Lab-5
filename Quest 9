#include<iostream>
#include<iomanip>
#include<stdlib.h>
#include<math.h>

using namespace std;

int main()
{
    float s[1000];
    int  n;
    cin >> n;
    float max = 0;
    for (int i = 0; i < n; i++) {
    	cin >> s[i];
	}
	for(int i = 0; i < n; i++) {
		if (max <= s[i]) { 
		max = s[i];	
		}
			}
		
	cout << setprecision(2) << fixed << max << endl;
	return 0;
}
