#include<iomanip>
#include<stdlib.h>
#include<iostream>
#include<math.h>
using namespace std;
int main() {

    int n;
    cin >> n;
    double s[1000];
    double sum = 0;
    for (int i = 0; i < n; i++) {
        cin >> s[i];
    }
    for (int i = 0; i < n; i++) {
	sum += s[i];
}

cout << setprecision(2) << fixed <<sum / n;
    return 0;
}
