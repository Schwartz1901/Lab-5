#include<iostream>
#include<iomanip>
#include<stdlib.h>
#include<math.h>


using namespace std;

int main()
{
	float pi =3.14;
	int x = 5;
	while( x <= 85) 
	{
		float a = x*pi/180;
		cout << setprecision(2) << fixed;
		cout << sin(a) << " ";
		cout << cos(a) << " ";
		cout << tan(a) << endl;
		x = x + 5;
	}
	return 0;
  
}
